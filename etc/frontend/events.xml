<?xml version="1.0"?>
<config
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd"
>
    <event name="layout_load_before">
        <observer
            name="mooore_wordpressintegrationcms"
            instance="Mooore\WordpressIntegrationCms\Observer\LayoutLoadBefore"
        />
    </event>
    <event name="hyva_config_generate_before">
        <observer
            name="Mooore_WordpressIntegrationCms"
            instance="Mooore\WordpressIntegrationCms\Observer\RegisterModuleForHyvaConfig"
        />
    </event>
</config>
